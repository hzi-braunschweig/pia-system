{
  "name": "eventhistoryserver",
  "version": "1.6.0",
  "description": "Stores and offers a history of events for enabled studies",
  "license": "AGPL-3.0-or-later",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "build.local": "npm run build",
    "prebuild.local": "npm run build.routes && npm run build.openapi",
    "start": "node src/index.js",
    "cli": "node src/cli.js",
    "test.int": "DOTENV_CONFIG_PATH=tests/test.env nyc mocha -r '../psa.utils.scripts/test.int/setup.hook' 'tests/**/*.spec.ts' --timeout 30000 --exit",
    "pretest.int": "npm run build.routes",
    "lint": "eslint .",
    "build.openapi": "tsoa spec",
    "build.routes": "tsoa routes",
    "postbuild.routes": "echo \"// @ts-nocheck\" > temp.ts && cat src/publicRoutes.generated.ts >> temp.ts && mv temp.ts src/publicRoutes.generated.ts",
    "typeorm": "typeorm-ts-node-commonjs"
  },
  "dependencies": {
    "@hapi/boom": "^10.0.0",
    "@hapi/hapi": "~21.1.0",
    "@keycloak/keycloak-admin-client": "^18.0.2",
    "@pia-system/lib-auth-server-client": "file:../psa.lib.auth-server-client",
    "@pia/lib-messagequeue": "file:../psa.lib.messagequeue",
    "@pia/lib-publicapi": "file:../psa.lib.publicapi",
    "@pia/lib-service-core": "file:../psa.lib.service-core",
    "commander": "^12.0.0",
    "date-fns": "^3.6.0",
    "http-status-codes": "^2.2.0",
    "joi": "^17.12.3",
    "pg": "^8.4.0",
    "reflect-metadata": "^0.2.2",
    "tsoa": "^6.2.0",
    "typeorm": "^0.3.20",
    "typeorm-naming-strategies": "^4.1.0"
  },
  "devDependencies": {
    "@pia/eslint-config": "file:../psa.eslint-config",
    "@types/amqplib": "^0.10.5",
    "@types/chai": "^4.3.4",
    "@types/chai-http": "^4.2.0",
    "@types/date-fns": "^2.6.0",
    "@types/mocha": "^10.0.0",
    "@types/node": "^20.3.0",
    "@types/node-fetch": "^2.6.2",
    "@types/sinon": "^10.0.13",
    "@types/sinon-chai": "^3.2.9",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.43.0",
    "amqplib": "^0.10.4",
    "chai": "^4.3.7",
    "chai-http": "^4.3.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.27.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-no-only-tests": "^3.1.0",
    "eslint-plugin-security": "^1.5.0",
    "mocha": "^10.1.0",
    "mocha-multi-reporters": "^1.5.1",
    "nyc": "^15.1.0",
    "sinon": "^14.0.2",
    "sinon-chai": "^3.7.0",
    "source-map-support": "^0.5.21",
    "ts-node": "10.9.1",
    "typescript": "~4.8.4"
  }
}
