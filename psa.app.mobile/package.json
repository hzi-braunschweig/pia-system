{
  "name": "app-mobile",
  "description": "PIA mobile app",
  "version": "1.37.4",
  "license": "AGPL-3.0-or-later",
  "scripts": {
    "prestart:browser": "npm run licenses",
    "start": "ng serve --port=4400",
    "start:android:emulator": "ionic cordova run android --emulator --livereload --consolelogs --external",
    "start:android:device": "ionic cordova run android --device --livereload --consolelogs --external",
    "start:ios:emulator": "ionic cordova run ios --emulator --livereload --consolelogs --external",
    "start:ios:device": "ionic cordova run ios --device --livereload --consolelogs --external",
    "prebuild": "npm run licenses",
    "build": "npx jetify && ng build --configuration production",
    "appflow:build": "npm run build",
    "preappflow:build": "cd ../psa.lib.licensecollector && npm ci",
    "prebuild:browser": "npm run licenses",
    "build:browser": "ng build --configuration production --base-href /assets/browser/ && cordova platform add browser && cordova build browser --release",
    "build:android": "ionic cordova build android --configuration production --release",
    "build:android:debug": "ionic cordova build android --debug",
    "build:ios": "ionic cordova build ios --configuration production --release",
    "test": "ng test",
    "test.unit": "ng test --no-watch --no-progress --browsers=ChromeHeadlessCI",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "licenses": "licensecollector --format json --target ./src/assets/licenses.json --onlyProduction --assertValidLicenseTexts --excludePackages '@pia-system/charts@1.0.0;@ngx-translate/core@15.0.0;@ngx-translate/http-loader@8.0.0'"
  },
  "private": true,
