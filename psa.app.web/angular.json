{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "psa": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        },
        "@schematics/angular:application": {
          "strict": true
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "preserveSymlinks": true,
            "allowedCommonJsDependencies": [
              "base64-js",
              "jspdf",
              "jspdf-autotable",
              "js-sha256",
              "keycloak-js",
              "luxon",
              "remove-markdown"
            ],
            "assets": [
              "src/favicon.png",
              "src/assets",
              "src/firebase-messaging-sw.js"
            ],
            "styles": [
              "src/custom-theme.scss",
              "src/styles.scss",
              "./node_modules/flag-icon-css/css/flag-icons.min.css",
              "./node_modules/swiper/swiper-bundle.css",
              "./node_modules/material-design-icons-iconfont/dist/material-design-icons.css"
            ],
            "scripts": ["./node_modules/marked/marked.min.js"]
          },
          "configurations": {
            "production-proband-app": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.proband.prod.ts"
                }
              ],
              "outputHashing": "all",
              "baseHref": "/",
              "outputPath": "./dist/out"
            },
            "production-admin-app": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.admin.prod.ts"
                }
              ],
              "outputHashing": "all",
              "baseHref": "/admin/",
              "outputPath": "./dist/out/admin"
            },
            "development-proband-app": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.proband.ts"
                }
              ],
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true,
              "baseHref": "/"
            },
            "development-admin-app": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.admin.ts"
                }
              ],
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true,
              "baseHref": "/admin/",
              "outputPath": "./dist/admin"
            }
          },
          "defaultConfiguration": "throw-on-default"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production-proband-app": {
              "browserTarget": "psa:build:production-proband-app"
            },
            "production-admin-app": {
              "browserTarget": "psa:build:production-admin-app"
            },
            "development-proband-app": {
              "browserTarget": "psa:build:development-proband-app",
              "proxyConfig": "proxy.conf.json"
            },
            "development-admin-app": {
              "browserTarget": "psa:build:development-admin-app",
              "proxyConfig": "proxy.conf.json"
            }
          },
          "defaultConfiguration": "throw-on-default"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "psa:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "preserveSymlinks": true,
            "codeCoverage": true,
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "inlineStyleLanguage": "scss",
            "assets": ["src/favicon.ico", "src/assets"],
            "styles": ["src/styles.scss"],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": ["src/**/*.ts", "src/**/*.html"]
          }
        },
        "cypress-run": {
          "builder": "@cypress/schematic:cypress",
          "options": {
            "devServerTarget": "psa:serve"
          },
          "configurations": {
            "production-proband-app": {
              "devServerTarget": "psa:serve:production-proband-app"
            },
            "production-admin-app": {
              "devServerTarget": "psa:serve:production-admin-app"
            }
          }
        },
        "cypress-open": {
          "builder": "@cypress/schematic:cypress",
          "options": {
            "devServerTarget": "psa:serve",
            "watch": true,
            "headless": false
          },
          "configurations": {
            "production-proband-app": {
              "devServerTarget": "psa:serve:production-proband-app"
            },
            "production-admin-app": {
              "devServerTarget": "psa:serve:production-admin-app"
            }
          }
        },
        "e2e": {
          "builder": "@cypress/schematic:cypress",
          "options": {
            "devServerTarget": "psa:serve",
            "watch": true,
            "headless": false
          },
          "configurations": {
            "production-proband-app": {
              "devServerTarget": "psa:serve:production-proband-app"
            },
            "production-admin-app": {
              "devServerTarget": "psa:serve:production-admin-app"
            }
          }
        }
      }
    }
  },
  "defaultProject": "psa",
  "cli": {
    "schematicCollections": ["@angular-eslint/schematics"]
  }
}
